struct Foo {}

struct User {
    id: u64;
    email: str;
    enabled: bool = true;

    fn hello() {
        println("Hello, World!");
    }

    fn greet(name: str) {
        println("Hello, {name}");
    }

    fn show_email(self) {
        println("{self.email}");
    }
}

fn main() {
    let foo: Foo = Foo {};
    let user: User = User {
        id: 1,
        email: "foo@bar.com",
    };

    println(foo);
    println(user);

    user.hello();
    user.greet("bar");
}

