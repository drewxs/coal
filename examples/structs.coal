struct User {
    id: u64;
    email: str;
    enabled: bool = true;

    fn hello() {
        println("Hello, World!");
    }

    fn greet(name: str) {
        print("Hello, ");
        println(name);
    }
}

fn main() {
    let u: User = User {
        id: 1,
        email: "foo@bar.com",
    };

    println(u);

    u.hello();
    u.greet("bar");
}

